{
    "include": ["src/**/*", "global.d.ts"],
    "compilerOptions": {
      "target": "ES2021",
      // Generate d.ts files
      "declaration": true,
      "outDir": "dist",
      "skipLibCheck": true,
      "sourceMap": true,
      "inlineSources": true,
      "strict": true,
      "module": "NodeNext",
		  "moduleResolution": "NodeNext",
      "experimentalDecorators": true,
      "jsx": "react-jsx",
		  "jsxImportSource": "@ui5/webcomponents-base",
    },
}
